<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
              version="1.2">

    <navigation-rule>
        <from-view-id>welcome_page.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>startPage</from-outcome>
            <to-view-id>start_page.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>start_page.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>mainPage</from-outcome>
            <to-view-id>main_page.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>easter_egg_page.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>mainPage</from-outcome>
            <to-view-id>main_page.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>main_page.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>startPage</from-outcome>
            <to-view-id>start_page.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>easterEgg</from-outcome>
            <to-view-id>easter_egg_page.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>